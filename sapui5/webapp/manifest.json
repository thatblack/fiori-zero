{
  "_version": "1.40.0",
  "sap.app": {
    "id": "ns.sapui5",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.7.3",
      "toolsId": "de7840b8-75ae-4a56-b331-75e803ad1d55"
    },
    "dataSources": {

      "mainOData": {
        "uri": "/V2/Northwind/Northwind.svc/",
        "type": "OData",
        "settings": {
            "annotations": [],
          "odataVersion": "2.0",
          "localUri": "localService/metadata.xml"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "ns.sapui5.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.102.1",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ns.sapui5.i18n.i18n"
        }
      },
      "invoicesLocal": {
        "_comment": "modelo local en archivo json Invoices.json",
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Invoices.json"
      },
      "northwind_Int": {
        "_comment": "modelo desde destination, si se cambia el datasouce se debe actualizar tambien en datasources arriba",
        "dataSource": "mainOData",
        "preload": true,
        "settings": {

        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "ns.sapui5.view",
        "controlAggregation": "pages",
        "controlId": "app_one",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteApp",
          "pattern": "",
          "target": [
            "TargetOverview"
          ]
        },
        {
            "name": "Details",
            "pattern": "detalles/{invoicePath}",
            "target": [
              "TargetDetails"
            ]  
        }
      ],
      "targets": {
        "TargetOverview": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Overview",
          "viewName": "Overview" 
        },
        "TargetDetails": {
            "viewType": "XML",
            "transition": "slide",
            "clearControlAggregation": false,
            "viewId": "Details",
            "viewName": "Details" 
          }
        
      }
    }
  }
}